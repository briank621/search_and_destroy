{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-1.12.0.min.js", "jquery.qtip.min.js", "content.js"],
      "css": ["jquery.qtip.min.css", "background.css"]
    }
  ],
  "background": {
    "scripts": ["jquery-1.12.0.min.js", "background.js"]
  }
}
